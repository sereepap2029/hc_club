RewriteEngine on
RewriteCond $1 !^(index\.php|css|assets|js|fonts|images|img|pdfs|captcha|media|robots\.txt|jquery-ui-1.12.1|ckeditor|vendors|bootstrap|ckfinder|flash|print_template|channel\.html)
RewriteRule ^(.*)$ /index.php/$1 [L]
RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /index\.php\ HTTP/
RewriteRule ^(.*)$ index.php/$1 [L,QSA] 
Header set Access-Control-Allow-Origin "*"